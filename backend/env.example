# =============================================================================
# RunPod Configuration - VLM (AIN Model for Vision/OCR)
# =============================================================================
RUNPOD_ENDPOINT_URL=https://api.runpod.ai/v2/YOUR_VLM_ENDPOINT_ID/runsync
RUNPOD_API_KEY=your_runpod_api_key_here

# =============================================================================
# Mistral API - LLM for Agentic OCR Reasoning (RECOMMENDED)
# =============================================================================
# Mistral API is the recommended LLM for agentic OCR. It's simpler to set up
# than self-hosting and has no cold start delays.
# Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_MODEL=mistral-large-latest  # Optional: mistral-medium, mistral-small

# =============================================================================
# RunPod Configuration - LLM (OPTIONAL, legacy)
# =============================================================================
# Alternative to Mistral: self-host Qwen 2.5 on RunPod
# Only used if MISTRAL_API_KEY is not set
# RUNPOD_LLM_ENDPOINT_URL=https://api.runpod.ai/v2/YOUR_LLM_ENDPOINT_ID/runsync
# RUNPOD_LLM_API_KEY=your_runpod_api_key_here

# =============================================================================
# Frontend URL (for CORS)
# =============================================================================
FRONTEND_URL=https://your-frontend-url.vercel.app

# =============================================================================
# Clerk Authentication
# =============================================================================
CLERK_SECRET_KEY=sk_test_your_secret_key_here

# =============================================================================
# Supabase (for OCR history and review queue storage)
# =============================================================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key_here

# =============================================================================
# Vercel Blob Storage (auto-configured by Vercel when connected)
# =============================================================================
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxxxx

# =============================================================================
# Server Configuration
# =============================================================================
PORT=8000

# =============================================================================
# Agentic OCR Settings (optional)
# =============================================================================
# AGENTIC_MAX_ITERATIONS=3
# AGENTIC_CONFIDENCE_THRESHOLD=0.85

